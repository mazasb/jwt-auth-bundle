{
    "name": "mazasb/jwt-auth-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JwtAuthBundle",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Balazs Mazas",
            "email": "mazasb@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/expression-language": "~2.8|~3.0",
        "mazasb/jwt-auth": "~1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.0, <5.4",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/security-bundle": "~2.8|~3.0",
        "symfony/browser-kit": "~2.8|~3.0",
        "mockery/mockery": "~0.9"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/mazasb/jwt-auth"}
    ],
    "autoload": {
        "psr-4": {
            "Acme\\JwtAuthBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
